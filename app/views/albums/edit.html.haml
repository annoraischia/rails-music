= form_with model: @album do |f|
    = f.label :title
    = f.text_field :title
  
    %br/
  
    = f.label :artist
    = f.text_field :artist
  
    %br/
  
    = f.label :type
    = f.text_field :type
  
    %br/

    = f.label :year
    = f.text_field :year
  
    %br/
    = f.label :genre
    = f.text_field :genre

    %br/
    = f.submit
  
    = link_to albums_path do
        Back to Albums